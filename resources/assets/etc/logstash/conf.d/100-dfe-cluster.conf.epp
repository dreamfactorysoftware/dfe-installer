input {
  gelf {
    type => "<%- $dc_index_type -%>"
    port => <%- $dc_port -%>
  }
}

filter {
  if [content] != "" {
    if [type] == "<%- $dc_index_type -%>" {
       json {
         source => "content"
         target => "payload"
         remove_field => [ "__dfUI" ]
       }
    }
  }
}

output {
  elasticsearch {
    host => "localhost"
    cluster => "<%- $es_cluster -%>"
    protocol => "http"
  }
}
